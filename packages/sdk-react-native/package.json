{
  "name": "@harkenapp/sdk-react-native",
  "version": "0.0.1-alpha.8",
  "description": "Harken React Native / Expo SDK for in-app feedback",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "react-native": "./src/index.ts",
  "source": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "react-native": "./src/index.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./attachments": {
      "types": "./dist/attachments/index.d.ts",
      "react-native": "./src/attachments/index.ts",
      "import": "./dist/attachments/index.js",
      "default": "./dist/attachments/index.js"
    },
    "./theme": {
      "types": "./dist/theme/index.d.ts",
      "react-native": "./src/theme/index.ts",
      "import": "./dist/theme/index.js",
      "default": "./dist/theme/index.js"
    },
    "./app.plugin.js": "./app.plugin.js",
    "./app.plugin": "./app.plugin.js"
  },
  "files": [
    "dist",
    "src",
    "app.plugin.js",
    "app.plugin.cjs"
  ],
  "scripts": {
    "generate": "openapi-typescript openapi.yaml -o src/types/openapi.ts",
    "build": "pnpm run generate && tsc",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm run clean && pnpm run build"
  },
  "keywords": [
    "react-native",
    "expo",
    "feedback",
    "sdk",
    "mobile",
    "harken"
  ],
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thutch-conecrow/harken.git",
    "directory": "packages/sdk-react-native"
  },
  "bugs": {
    "url": "https://github.com/thutch-conecrow/harken/issues"
  },
  "homepage": "https://github.com/thutch-conecrow/harken#readme",
  "sideEffects": false,
  "expo": {
    "configPlugin": "./app.plugin.cjs"
  },
  "peerDependencies": {
    "@react-native-async-storage/async-storage": ">=1.21.0",
    "@react-native-community/netinfo": ">=11.0.0",
    "expo": ">=54.0.0 <55.0.0",
    "expo-document-picker": ">=12.0.0",
    "expo-file-system": ">=19.0.0",
    "expo-image-picker": ">=15.0.0",
    "expo-secure-store": ">=13.0.0",
    "react": ">=18.0.0",
    "react-native": ">=0.72.0",
    "react-native-safe-area-context": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "react-native": {
      "optional": false
    },
    "expo": {
      "optional": true
    },
    "expo-secure-store": {
      "optional": true
    },
    "expo-file-system": {
      "optional": true
    },
    "expo-image-picker": {
      "optional": true
    },
    "expo-document-picker": {
      "optional": true
    },
    "@react-native-async-storage/async-storage": {
      "optional": true
    },
    "@react-native-community/netinfo": {
      "optional": true
    },
    "react-native-safe-area-context": {
      "optional": true
    }
  },
  "devDependencies": {
    "@react-native-async-storage/async-storage": "^2.1.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@testing-library/react": "^16.3.2",
    "@types/react": "^19.1.0",
    "@vitest/coverage-v8": "^4.0.17",
    "expo-document-picker": "^14.0.0",
    "expo-file-system": "^19.0.0",
    "expo-image-picker": "^17.0.0",
    "jsdom": "^27.4.0",
    "openapi-typescript": "^7.4.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-native": "^0.81.5",
    "react-native-safe-area-context": "^5.0.0",
    "typescript": "^5.9.0",
    "vitest": "^4.0.17"
  }
}
