{
  "name": "@harken/sdk-react-native",
  "version": "0.0.1",
  "description": "Harken React Native / Expo SDK for in-app feedback",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "react-native": "./src/index.ts",
  "source": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "react-native": "./src/index.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./theme": {
      "types": "./dist/theme/index.d.ts",
      "react-native": "./src/theme/index.ts",
      "import": "./dist/theme/index.js",
      "default": "./dist/theme/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm run clean && pnpm run build"
  },
  "keywords": [
    "react-native",
    "expo",
    "feedback",
    "sdk",
    "mobile",
    "harken"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/harken-app/harken.git",
    "directory": "packages/sdk-react-native"
  },
  "bugs": {
    "url": "https://github.com/harken-app/harken/issues"
  },
  "homepage": "https://github.com/harken-app/harken#readme",
  "sideEffects": false,
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-native": ">=0.72.0",
    "expo-secure-store": ">=13.0.0"
  },
  "peerDependenciesMeta": {
    "react-native": {
      "optional": false
    },
    "expo-secure-store": {
      "optional": true
    }
  },
  "dependencies": {
    "@harken/contracts": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "^18.3.18",
    "@types/react-native": "^0.72.8",
    "react": "^18.3.1",
    "react-native": "^0.76.6",
    "typescript": "^5.7.2"
  }
}
